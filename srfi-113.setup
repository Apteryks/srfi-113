;; -*- Hen -*-

(define (dynld-name fn)
  (make-pathname #f fn ##sys#load-dynamic-extension))

(compile -O3 -d0 -s -J sets/sets.scm -o srfi-113.so)
(compile -O2 -d0 -s srfi-113.import.scm)

(install-extension
 'srfi-113
 `(,(dynld-name "srfi-113") ,(dynld-name "srfi-113.import"))
 '((version "0.3")))
